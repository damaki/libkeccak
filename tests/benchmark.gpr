with "libkeccak";

project Benchmark is

   for Source_Dirs use (".");
   for Object_Dir use "out/benchmark";
   for Main use ("benchmark.adb");

   package Compiler is
      for Default_Switches ("Ada") use ("-Winline", "-O2", "-gnatN", "-gnatp", "-gnatwe");
   end Compiler;
   
   package Linker is
      for Default_Switches ("Ada") use ("-Wl,--gc-sections");
   end Linker;

end Benchmark;

